        "goto": "set_hand"
      },
      {
        "text": "거리를 둔다.",
        "goto": "set_distance"
      }
    ]
  },
  {
    "id": "set_hand",
    "type": "set",
    "var": "hand",
    "value": 1,
    "next": "to_end"
  },
  {
    "id": "set_distance",
    "type": "set",
    "var": "distance",
    "value": 1,
    "next": "to_end"
  },
  {
    "id": "to_end",
    "type": "jump",
    "goto": "end_selector"
  },
  {
    "id": "end_selector",
    "type": "if",
    "cases": [
      {
        "cond": "hand==1 && honest==1",
        "goto": "end_happy"
      },
      {
        "cond": "hand==1 && avoid==1",
        "goto": "end_normal"
      },
      {
        "cond": "distance==1 && avoid==1",
        "goto": "end_bad"
      },
      {
        "cond": "distance==1 && honest==1",
        "goto": "end_bittersweet"
      }
    ],
    "else": "end_secret"
  },
  {
